{
    "name": "apimatic/yapstonestagedonboardingservices",
    "description": "APIs for onboarding and managing customers and instruments.",
    "type": "library",
    "keywords": [
        "Yapstone Staged On-Boarding Services",
        "API",
        "SDK"
    ],
    "homepage": "https://www.apimatic.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Yapstone",
            "email": "support@yapstone.com"
        }
    ],
    "require": {
        "php": ">=7.2 <8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "apimatic/unirest-php": "^3.0.1",
        "apimatic/jsonmapper": "^3.0.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phan/phan": "5.3.1"
    },
    "autoload": {
        "psr-4": {
            "YapstoneStagedOnBoardingServicesLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YapstoneStagedOnBoardingServicesLib\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs --standard=phpcs-ruleset.xml src/",
        "lint-fix": "phpcbf --standard=phpcs-ruleset.xml src/",
        "analyze": "phan --allow-polyfill-parser"
    }
}